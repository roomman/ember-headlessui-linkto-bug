{{page-title 'EmberHeadlessuiLinktoBug'}}
<Menu as |menu|>
  <menu.Button>
    Menu Trigger
  </menu.Button>
  <menu.Items as |items|>
    {{#let (component 'link-to' route='foo') as |Link|}}
      <items.Item as |item|>
        <item.Element @tagName={{Link}}>
          Take me to Foo
        </item.Element>
      </items.Item>
    {{/let}}
  </menu.Items>
</Menu>

{{outlet}}